  "private": true,
  "homepage": "http://localhost:3000",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/reportWebVitals.js",
      "!src/setupTests.js",
      "!src/**/*.test.{js,jsx}",
      "!src/**/__tests__/**/*"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 60,
        "functions": 60,
        "lines": 60,
        "statements": 60
      }
    },
    "moduleNameMapper": {
      "^axios$": "axios/dist/node/axios.cjs"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(axios|react-hot-toast)/)"
    ]
  }
}
